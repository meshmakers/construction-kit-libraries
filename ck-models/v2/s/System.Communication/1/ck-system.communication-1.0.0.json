    },
    {
      "id": "SapClient-1",
      "valueType": "String",
      "description": "A client is a tenant instance in an SAP system. SAP is multi-tenant, which means that multiple organizations or departments can work on the same system without sharing data with each other."
    },
    {
      "id": "SapGatewayHost-1",
      "valueType": "String",
      "description": "An SAP gateway host is a server that provides communication services between SAP systems or between SAP systems and external applications."
    },
    {
      "id": "SapGatewayService-1",
      "valueType": "String",
      "description": "An SAP gateway service provides communication services that are used between SAP systems or between SAP systems and external applications."
    },
    {
      "id": "SapLanguage-1",
      "valueType": "String",
      "description": "The language key is a two-character code that identifies the language in which the user interface is displayed."
    },
    {
      "id": "SapProgramId-1",
      "valueType": "String",
      "description": "A program ID is a unique identifier that is used to register a program on an SAP gateway server."
    },
    {
      "id": "SapRegistrationCount-1",
      "valueType": "Int",
      "description": "The registration count is the number of times a program is registered on an SAP gateway server."
    },
    {
      "id": "SapTrace-1",
      "valueType": "String",
      "defaultValues": [
        "8"
      ],
      "description": "The trace level is a number that determines the amount of information that is logged during communication with the SAP system."
    },
